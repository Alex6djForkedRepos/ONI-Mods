<Project Sdk="Microsoft.NET.Sdk">

  <!-- General Package Properties -->
  <PropertyGroup>
    <PackageId>TwelveCyclesOfChristmas</PackageId>
    <Version>1.0.0</Version>
    <Authors>Aki</Authors>
    <Copyright>2021 Aki</Copyright>
    <RepositoryUrl>https://github.com/romen-h/ONI-Mods</RepositoryUrl>
  </PropertyGroup>

  <!-- Build Properties -->
  <PropertyGroup>
    <TargetFramework>net471</TargetFramework>
    <Configurations>Debug</Configurations>
    <AssemblyName>TwelveCyclesOfChristmas</AssemblyName>
    <RootNamespace>TwelveCyclesOfChristmas</RootNamespace>
    <IsMod>true</IsMod>
    <GenerateMetadata>true</GenerateMetadata>
    <FUtility>true</FUtility>
    <IsPacked>true</IsPacked>
  </PropertyGroup>

  <!-- Mod Info -->
  <PropertyGroup>
    <ModName>Twelve Cycles Of Christmas</ModName>
    <ModDescription>desc</ModDescription>
    <SupportedContent>ALL</SupportedContent>
    <MinimumSupportedBuild>469369</MinimumSupportedBuild>
    <APIVersion>2</APIVersion>
  </PropertyGroup>
  
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutDir>bin</OutDir>
  </PropertyGroup>
  
  <ItemGroup>
    <ProjectReference Update="..\FUtility\FUtility.csproj">
      <Private>true</Private>
    </ProjectReference>
  </ItemGroup>
  
</Project>
